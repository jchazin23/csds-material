replace col: Team on: `\\` with: '' global: true
replace col: Team on: `[[\#2\|^]]` with: '' global: true
replace col: Team on: `[[\#1\|\*]]` with: '' global: true
replace col: Team on: `<sup>[[\#3\|\#]]</sup>` with: '' global: true
split col: Team on: '\n|' limit: 5
replace col: Team1 on: `\{\{fb\|` with: '' global: true
replace col: Team1 on: `\}\}` with: '' global: true
drop col: Titles
drop col: Runners_up
drop col: Third_place
drop col: Fourth_place
drop col: Top_4_br_finishes
drop col: Team6
replace col: Team2,Team3,Team4,Team5 on: `{start}{digit} ` with: ''
